package entities;

public class Product {

	private Integer id;
	private String nome;
	private Double salary;
	
	public Product() {
	}

	public Product(Integer id, String nome, Double salary) {
		this.id = id;
		this.nome = nome;
		this.salary = salary;
	}

	//Getters and Setters
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Double getSalary() {
		return salary;
	}

	public void setSalary(Double salary) {
		this.salary = salary;
	}
	
	
	
	
	
	
}
